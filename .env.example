# URL base do site (usado para SEO, sitemap, e OG images)
NEXT_PUBLIC_SITE_URL=

# Analytics (opcional)
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Umami Cloud Analytics (opcional - https://cloud.umami.is)
# Crie uma conta gratuita em https://cloud.umami.is e adicione seu site
# O Website ID está disponível em Settings > Website > Website ID
NEXT_PUBLIC_UMAMI_WEBSITE_ID=
NEXT_PUBLIC_UMAMI_URL=https://cloud.umami.is

# Giscus Comments (opcional - https://giscus.app)
# Configure em https://giscus.app
# 1. O repositório deve ser público
# 2. O app Giscus deve estar instalado no repositório
# 3. Discussions devem estar habilitadas no repositório
NEXT_PUBLIC_GISCUS_REPO=
NEXT_PUBLIC_GISCUS_REPO_ID=
NEXT_PUBLIC_GISCUS_CATEGORY=
NEXT_PUBLIC_GISCUS_CATEGORY_ID=

# Verificação de search engines (opcional)
# GOOGLE_SITE_VERIFICATION=xxxxxxxxxxxxx
# BING_SITE_VERIFICATION=xxxxxxxxxxxxx
